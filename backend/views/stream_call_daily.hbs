<html>
    <script crossorigin src="https://unpkg.com/@daily-co/daily-js"></script>
    <body>
        <script>
        callFrame = window.DailyIframe.createFrame({
            iframeStyle: {
                position: 'fixed',
                top: 0,
                left: 0,
                width: '100%',
                height: '100%',
            },
        });
        callFrame.join({ url: "{{dailyDomain}}/{{room}}", token: "{{token}}" });
        </script>
    </body>
</html>
